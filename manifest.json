{
   "background": {
      "scripts": [ "libs/jquery.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "assets/icon/icon-128.png",
         "19": "assets/icon/icon-19.png",
         "38": "assets/icon/icon-38.png",
         "48": "assets/icon/icon-48.png"
      },
      "default_popup": "popup.html",
      "default_title": "__MSG_title__"
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "*://*/*" ]
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "default_locale": "ru",
   "icons": {
      "128": "assets/icon/icon-128.png",
      "16": "assets/icon/icon-16.png",
      "32": "assets/icon/icon-32.png",
      "48": "assets/icon/icon-48.png"
   },
   "manifest_version": 2,
   "name": "Audio NIR",
   "short_name" : "Audio",
	"version" : "1.0",
	"author" : "Lobanoff",
"description" : "Тестовый пример работы расширения для Google Chrome в виде плагина перехвата и усиления или уменьшения громкости звука. By Lobanoff Artem 2020",
   "permissions": [ "tabCapture", "activeTab", "tabs", "storage", "notifications", "http://*/", "https://*/", "webRequest", "webRequestBlocking" ],
   "short_name": "__MSG_short_name__"
}
